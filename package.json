{
  "name": "email-builder",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "start": "snowpack dev",
    "babel-transpile": "babel src -d lib && babel public --out-dir build --copy-files",
    "generate": "node generator.mjs",
    "prebuild": "rm -rf lib && rm -rf build",
    "build": "mkdir build && npm run babel-transpile && node --experimental-modules --es-module-specifier-resolution=node generator.mjs"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@snowpack/plugin-react-refresh": "^2.5.0",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-flowtype": "^5.7.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "js-beautify": "^1.10.1-rc2",
    "prettier": "^2.3.1",
    "promise": "^8.1.0",
    "snowpack": "^3.3.7"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  }
}
